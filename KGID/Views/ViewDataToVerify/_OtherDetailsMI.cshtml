                <label class="control-label text-justify">
                    <b>
                        <span class='slnoid1 slnoid2'>slno</span>. Do you wish to include P.A. Cover for unnamed persons / hire / pillion passenger (two wheelers)?
                    </b> :
                </label>
            </div>
            <div class="form-group col-2">
                @if (Model.miod_is_include_pa_cover_for_unnamed_persons == true)
                {
                    <span class="spvehicleCondition col-11">Yes</span>
                }
                else
                {
                    <span class="spnvehicleCondition col-11">No</span>
                }
            </div>
            <br />
        </div>
        <div class="pa-for-un-persons-details">
            <div class="row">
                <div class="form-group col-4">
                    <b>If Yes, give name and Capital Sum Insured(CSI) opted for.The maximum CSI available per person is Rs.2 Lakhs in the case of private cars and Rs.1 Lakh in the case of motorised two wheelers </b>
                </div>
                <div class="form-group col-4">
                    <strong>Numbers of Persons</strong>
                </div>
                <div class="form-group col-4">
                    <strong>CSI Opted(Rs)</strong>
                </div>
            </div>
            <div class="row">
                <div class="form-group col-4" style="text-align:center">
                    1).
                </div>
                <div class="form-group col-4">
                    @Html.DisplayFor(model => model.miod_ipaun_name1)

                </div>
                <div class="form-group col-4">
                    @Html.DisplayFor(model => model.miod_ipaun_name1_amount)

                </div>
            </div>
            <div class="row">
                <div class="form-group col-4" style="text-align:center">
                    2).
                </div>
                <div class="form-group col-4">
                    @Html.DisplayFor(model => model.miod_ipaun_name2)

                </div>
                <div class="form-group col-4">
                    @Html.DisplayFor(model => model.miod_ipaun_name2_amount)

                </div>
            </div>
            <div class="row">
                <div class="form-group col-4" style="text-align:center">
                    3).
                </div>
                <div class="form-group col-4">
                    @Html.DisplayFor(model => model.miod_ipaun_name3)

                </div>
                <div class="form-group col-4">
                    @Html.DisplayFor(model => model.miod_ipaun_name3_amount)

                </div>
            </div>

        </div>
        @if (Model.miod_vehicle_type_id == 2)
        {
            <div class="row">
                <div class="form-group col-10">
                    <label class="control-label text-justify">
                        <b>
                            <span class='slnoid1 slnoid2'>slno</span>. Is the vehicle fitted with the any Anti-theft device approved by the AARI. Pune?<br />
                            If yes,attach Certificate of Installation in the Vehicle issued by Automobile Association of India.
                        </b> :
                    </label>
                </div>
                <div class="form-group col-2">
                    @if (Model.miod_is_anti_theft == true)
                    {
                        <span class="spvehicleCondition col-11">Yes</span>
                    }
                    else
                    {
                        <span class="spnvehicleCondition col-11">No</span>
                    }
                </div>
                <br />
            </div>
            <div class="antitheft-details">
                <div class="form-group col-6 pl-0 pr-0">
                    @if (!string.IsNullOrEmpty(Model.Miod_is_anti_theft_doc_filename))
                    {
                        <label class="col-12 prevdoc"><b>Previously uploaded Document</b> : <a href="@Model.Miod_is_anti_theft_doc_filename" target="_blank" class="doc-uploaded">Click Here</a> </label>
                        @*@Html.TextBoxFor(m => m.PlaceChangeDocFileName, new { @class = "display-as-label form-label col-12 doc-uploaded" })*@
                        @*<a href="@Model.Miod_is_no_claim_bonus_doc_filename" target="_blank" class="doc-uploaded">Click Here</a>*@
                    }
                    @Html.DisplayFor(m => m.miod_is_anti_theft_doc)

                </div>
            </div>
        }


    </form>

</div>


<script>
    $(document).ready(function () {
        var geo = '@Model.miod_is_geographical';
        var ncb = '@Model.miod_is_no_claim_bonus';
         var antitheft = '@Model.miod_is_anti_theft';
        var biofuel = '@Model.miod_is_bi_fuel_system';
        var hd = '@Model.miod_is_higher_deductible';
        var htc = '@Model.miod_is_higher_towing_charges';
        var aai = '@Model.miod_is_automobile_association_of_india';
        var cll = '@Model.miod_is_cover_legal_liability';
        var ipa = '@Model.miod_is_include_personal_accident';
        var ipap = '@Model.miod_is_include_personal_accident_for_persons';
        var ipaun = '@Model.miod_is_include_pa_cover_for_unnamed_persons';
        //alert(name);
        //$('#hdnMIVDReferanceNo').val() = $('#spnMIReferanceNo').text();
        if (geo == "True") {
            $(".geographical-details").show();
        }
        else {
            $(".geographical-details").hide();
        }
        if (ncb == "True") {
            $(".no-claim-bonus-details").show();
        }
        else {
            $(".no-claim-bonus-details").hide();
        }
        if (biofuel == "True") {
            $(".bi-fuel-system-details").show();
        }
        else {
            $(".bi-fuel-system-details").hide();
        }
        if (hd == "True") {
            $(".higher-deductible-details").show();
        }
        else {
            $(".higher-deductible-details").hide();
        }
        if (htc == "True") {
            $(".higher-towing-charges-details").show();
        }
        else {
            $(".higher-towing-charges-details").hide();
        }
        if (aai == "True") {
            $(".automobile-association-details").show();
        }
        else {
            $(".automobile-association-details").hide();
        }
        if (cll == "True") {
            $(".legal_liability-details").show();
        }
        else {
            $(".legal_liability-details").hide();
        }
        if (ipa == "True") {
            $(".personal-accident-details").show();
        }
        else {
            $(".personal-accident-details").hide();
        }
        if (ipap == "True") {
            $(".pa-for-persons-details").show();
        }
        else {
            $(".pa-for-persons-details").hide();
        }
        if (ipaun == "True") {
            $(".pa-for-un-persons-details").show();
        }
        else {
            $(".pa-for-un-persons-details").hide();
        }

        if (antitheft == "True") {
            $(".antitheft-details").show();
        }
        else {
            $(".antitheft-details").hide();
        }
    });

</script>
