                        }
                        @if (Model.miod_geographical_ext2 == "3")
                        {
                            <label style="left:20px"><b>(i) Second Country:</b> Maldives </label>
                        }
                        @if (Model.miod_geographical_ext2 == "4")
                        {
                            <label style="left:20px"><b>(i) Second Country:</b> Nepal </label>
                        }
                        @if (Model.miod_geographical_ext2 == "5")
                        {
                            <label style="left:20px"><b>(i) Second Country:</b> Pakistan </label>
                        }
                        @if (Model.miod_geographical_ext2 == "6")
                        {
                            <label style="left:20px"><b>(i) Second Country:</b> Srilanka </label>
                        }
