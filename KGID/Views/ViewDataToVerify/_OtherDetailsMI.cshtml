            <p style="text-align: center;"><strong>PROPOSAL FORM-I</strong></p>
        }
        else
        {
            <p style="text-align: center;"><strong>PROPOSAL FORM - II </strong></p>
        }
        @Html.Hidden("miod_emp_id", Session["UId"], new { id = "hdnmivdEmpID" })
        @*@Html.Hidden("miod_application_id", new { id = "spnMIReferanceNo" })*@
        @Html.HiddenFor(m => m.miod_application_id, new { @id = "hdnMIReferanceNo" })
        @Html.HiddenFor(m => m.miod_vehicle_type_id, new { @id = "hdnVehicleTypeId" })

        <span class='slnoid1 slnoid2' style="display:none;">1</span>
        <div class="row">
            <div class="form-group col-10">
                <label class="control-label text-justify"><b><span class='slnoid1 slnoid2'>slno</span>. Whether the vehicle is driven by non-conventional source of power(if Yes, Please give details)<br />Permitted by the concerned RTA </b> : </label>
            </div>
            <div class="form-group col-2">
                @if (Model.miod_is_non_conventioanal_source == true)
