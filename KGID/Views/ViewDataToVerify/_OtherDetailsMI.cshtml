                        @if (Model.miod_geographical_ext3 == "1")
                        {
                            <label style="left:20px"><b>(i) Third Country:</b> Bangladesh </label>
                        }
                        @if (Model.miod_geographical_ext3 == "2")
                        {
                            <label style="left:20px"><b>(i) Third Country:</b> Bhutan </label>
                        }
                        @if (Model.miod_geographical_ext3 == "3")
                        {
                            <label style="left:20px"><b>(i) Third Country:</b> Maldives </label>
                        }
                        @if (Model.miod_geographical_ext3 == "4")
                        {
                            <label style="left:20px"><b>(i) Third Country:</b> Nepal </label>
                        }
                        @if (Model.miod_geographical_ext3 == "5")
                        {
                            <label style="left:20px"><b>(i) Third Country:</b> Pakistan </label>
                        }
                        @if (Model.miod_geographical_ext3 == "6")
                        {
                            <label style="left:20px"><b>(i) Third Country:</b> Srilanka </label>
                        }
