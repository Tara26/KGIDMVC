        @*// @Html.HiddenFor(x => x.Exam_Notif_Number)*@

        <div class="tab-pane fade" id="pills-home" role="tabpanel" aria-labelledby="pills-menu1-tab" style="color:black; ">

            <div id="workflowTab" class="application-tab">
                <section>
                    <table class="table table-striped table-bordered" id="tblCWWorkflowgrid" cellspacing="0">
                        <thead style="background: #308385;color: #fff;">
                            <tr>
                                @*<th>Application Referance Number</th>*@
                                <th>Submitted Date</th>
                                <th>From</th>
                                <th>To</th>
                                <th>Remarks</th>
                                <th>Comments</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model.MvcClaimWorkFlowDetails != null && Model.MvcClaimWorkFlowDetails.Count > 0)
                            {
                                foreach (var item in Model.MvcClaimWorkFlowDetails)
                                {
                                    <tr>
                                        @*<td>@item.ApplicationRefNo</td>*@
                                        <td>@item.SubmissionDate</td>
                                        @if (item.vehicle_category_desc == "")
                                        {
                                            <td>Agency</td>
                                        }
                                        else
                                        {
                                            <td>@item.From</td>
                                        }
                                        <td>@item.Tooo</td>
                                        <td>@item.Remarks</td>
                                        <td>@item.comments</td>
                                        <td>Pending</td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </section>
            </div>
            <div class="card-footer">
                <div class="row">
                    <div class="form-group col-12 text-right">
                        <a id="btnWFNext" class="btn btn-primary btn-sm text-white">Next</a>
                    </div>
                </div>
            </div>

