    <div class="lms-registration-form" id="TeacherDetails">
        <ul class="nav nav-pills mb-3 mt-4" id="pills-tab" role="tablist">
            <li class="nav-item">
                <a class="nav-link active" id="pills-home-tab" data-toggle="pill" href="#pills-home" role="tab" aria-controls="pills-home" aria-selected="true">WorkFlow Details</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="pills-menu1-tab" data-toggle="pill" href="#pills-menu1" role="tab" aria-controls="pills-menu1" aria-selected="false">Application View </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="pills-menu2-tab" data-toggle="pill" href="#pills-menu2" role="tab" aria-controls="pills-menu2" aria-selected="false">Scrutiny</a>
            </li>
        </ul>
    </div>

    <div class="tab-content" id="pills-tabContent">
