    function GetDocumentDetailsStatus(GetStatusDataDetails, Mvc_appId) {
        debugger;


        $.ajax({
            url: '/MotorInsuranceClaims/GetDocumentStatus',
            async: false,
            type: 'POST',
            cache: false,
            data: JSON.stringify({ GetStatusData: GetStatusDataDetails, appId: Mvc_appId }),
            processData: false,
            contentType: 'application/json; charset=utf-8',

            success: function (response) {
                $("#Tbl_doc_statusBody").empty();
                $("#myModal1").modal("show");
                if (response.GetDocumentRemarksList.length > 0) {
                    $.each(response.GetDocumentRemarksList, function (id, col) {
                        var markup = "<tr><td>" + dateFormat(new Date(parseInt((col.SubmissionDate).match(/\d+/)[0]))) + "</td><td>" + col.From + "</td><td>" + col.Tooo+"</td><td>" + col.Remarks + "</td><td>" + col.comments +"</td><td>Pending</td></tr>";

                        $("#Tbl_doc_statusBody").append(markup);
                    });
                } else {
                    alertify.alert("No record found.");
                }
            }
        });




    }
    function dateFormat(d) {
