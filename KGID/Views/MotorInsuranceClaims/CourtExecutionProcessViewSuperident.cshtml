                                <th>Address</th>
                                <th>Pincode</th>
                            </tr>
                        </thead>
                        <tbody id="petitioner_table_body">
                            @foreach (var data in Model.PetitionerList)
                            {
                                <tr>
                                    <td>@(slno++)</td>
                                    <td>@data.petitioner_name</td>
                                    <td>@data.petitioner_Mobile_no</td>
                                    <td>@data.petitioner_Address</td>
                                    <td>@data.pincode_Of_Petitioner</td>
