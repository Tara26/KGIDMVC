    ViewBag.Title = "MvcApplicationForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int slno = 1;
}

<style>
    button#btnApplicationVehicleSearch {
        padding: 10px 10px;
    }

    button#btnsend {
        padding: 10px 10px;
    }

    <style type="text/css" >
    /*.stat-style {
        margin: 10px;
        border: 1px solid #d8d8d8;
        border-radius: 10px;
        box-shadow: 0px 2px 6px 0px #8cbad2;
        height: 100%;
    }*/
