    function viewMVCdetailsModal(chassisno, Application_no) {
        debugger;

        $.ajax({
            url: '/MotorInsuranceClaims/MvcClaimChangesEdit',
            async: false,
            type: 'POST',
            cache: false,
            data: JSON.stringify({ chassis: chassisno, Appno: Application_no }),
            contentType: false,
            processData: false,
            contentType: 'application/json; charset=utf-8',
            success: function (response) {
                if (response.GetVehicleChassisPolicyDetailsList.length > 0) {

                    //  var odToDate = (response.GetVehicleChassisPolicyDetailsList[0].OD_to_date);

                    $('#VehicleRegistrationNumber1').val(response.GetVehicleChassisPolicyDetailsList[0].vehicle_registration_no);
                    $('#VehicleChassisNumber1').val(response.GetVehicleChassisPolicyDetailsList[0].vehicle_chasis_no);
                    $('#VehicleCategoryType1').val(response.GetVehicleChassisPolicyDetailsList[0].Vehicle_Category_Type);
                    $('#VehicleCategory1').val(response.GetVehicleChassisPolicyDetailsList[0].vehicle_category_desc);
                    $('#VehicleModel1').val(response.GetVehicleChassisPolicyDetailsList[0].vehicle_model);
                    $('#VehicleType1').val(response.GetVehicleChassisPolicyDetailsList[0].Vehicle_Type);
                    $('#txtTypeofCover1').val(response.GetVehicleChassisPolicyDetailsList[0].type_of_Cover);
                    $('#txtPolicyNo1').val(response.GetVehicleChassisPolicyDetailsList[0].Policy_number);
                    $('#ODFromDate1').val(response.GetVehicleChassisPolicyDetailsList[0].OD_from_date1);
                    $('#ODToDate1').val(response.GetVehicleChassisPolicyDetailsList[0].OD_to_date1);
                    $('#TPFromDate1').val(response.GetVehicleChassisPolicyDetailsList[0].TP_from_date1);
                    $('#TPToDate1').val(response.GetVehicleChassisPolicyDetailsList[0].TP_to_date1);

                }
