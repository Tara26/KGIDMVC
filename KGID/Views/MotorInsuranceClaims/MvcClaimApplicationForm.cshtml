<div class="card-body">

    <div class="lms-registration-form" id="TeacherDetails">
        <ul class="nav nav-pills mb-3 mt-4" id="pills-tab" role="tablist">
            <li class="nav-item">
                <a class="nav-link active" id="pills-home-tab" data-toggle="pill" href="#pills-home" role="tab" aria-controls="pills-home" aria-selected="true">Application Status </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="pills-menu1-tab" data-toggle="pill" href="#pills-menu1" role="tab" aria-controls="pills-menu1" aria-selected="false">New MVC Application form </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="pills-menu2-tab" data-toggle="pill" href="#pills-menu2" role="tab" aria-controls="pills-menu2" aria-selected="false">Lokadhalat </a>
            </li>
        </ul>
    </div>


    <div class="tab-content" id="pills-tabContent">

        <!----------------------------------------Application Status------------------->
        <div class="tab-pane fade show active" id="pills-home" role="tabpanel" aria-labelledby="pills-home-tab">
            @*<label style="color:black">Please read the following the instructions carefully. / ದಯವಿಟ್ಟು ಕೆಳಗಿನ ಸೂಚನೆಗಳನ್ನು ಎಚ್ಚರಿಕೆಯಿಂದ ಓದಿ. </label>*@
            <section>
                <div class="row">
                    <div class="col-12">

                        <table class="table table-striped table-bordered" id="tblSIVerifyData" cellspacing="0">
                            <thead style="background: #308385;color: #fff;">
                                <tr>

                                    <th>SL.No</th>
                                    <th>MVC No.</th>
                                    <th>Vehicle Chassis No</th>
                                    <th>Status</th>
                                    <th>Action</th>
                                    <th>Other Action</th>
                                </tr>
                            </thead>
                            <tbody>

                                @if (Model.GetVehicleChassisPolicyDetailsList != null && Model.GetVehicleChassisPolicyDetailsList.Count > 0)
                                {
                                    foreach (var item in Model.GetVehicleChassisPolicyDetailsList)
                                    {
                                        <tr>
                                            <td>@(slno++)</td>
                                            <td>@item.Court_MVC_number</td>
                                            <td>@item.vehicle_chasis_no</td>
                                            <td>Pending</td>
                                            <td>
                                                <a href="/mvc_cw_va/@item.vehicle_chasis_no/@item.MVC_claim_app_id">View</a>
                                            </td>
                                            <td>
                                                <a href='#' title='view' class='btn btn-primary' onclick='viewMVCdetailsSup("@item.vehicle_chasis_no","@item.MVC_claim_app_id")' name="View">Lokadhalat</a> <a href='#' title='view' class='btn btn-primary' onclick='viewMVCdetailsSup("@item.vehicle_chasis_no","@item.MVC_claim_app_id")' name="View">Court Execution</a>

                                            </td>
                                        </tr>
                                    }
                                }

                            </tbody>
                        </table>
                    </div>
                </div>

            </section>
        </div>

        <!-----------------------------------------New Application---------------------------->
        <div class="tab-pane fade" id="pills-menu1" role="tabpanel" aria-labelledby="pills-menu1-tab" style="color:black; ">
            <div class="row">
                <div class="col-md-2">
                </div>
                <div class="col-md-7">
                    <div class="form-group row">
                        <label class="col-sm-6 col-form-label align-center">
                            Enter Vehicle No./Chassis No./Policy No. :
                        </label>
                        <div class="col-sm-6">
                            <input type="text" class="form-control alphaOnly" id="txtVehiclePolicyChassisNo" required>
                        </div>
                    </div>
                </div>
                <div class="col-md-2">
                    <button class="btn-success" id="btnApplicationVehicleSearch">Search </button>
                </div>
                <div class="col-md-1">
                </div>
            </div>
            <h6 class="m-0 font-weight-bold text-primary mt-3"><b>Vehicle Details</b></h6>
            <form>
                <div class="card shadow-sm rounded p-3 mt-2">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group row">
                                <label class="col-sm-4 col-form-label align-center">
                                    Vehicle Reg No. :
                                </label>
                                <div class="col-sm-6">
                                    <input type="text" class="form-control alphaOnly" id="VehicleRegistrationNumber" required readonly>
                                    <span id="lblErrorTeacherName" , style="color:red;display:none;">Please Enter Teacher Name</span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group row">
                                <label class="col-sm-4 col-form-label align-center">
