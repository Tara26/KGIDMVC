    <div class="tab-content" id="pills-tabContent" style="background: #dfddd8;padding: 2.5%!important;">
        <div class="tab-pane fade show active" id="workflow" role="tabpanel" aria-labelledby="nav-home-tab">
            <div id="workflowTab" class="application-tab">
                <section>
                    <table class="table table-striped table-bordered" id="tblCWWorkflowgrid" cellspacing="0">
                        <thead style="background: #308385;color: #fff;">
                            <tr>
                                @*<th>Application Referance Number</th>*@
                                <th>Submitted Date</th>
                                <th>From</th>
                                <th>To</th>
                                <th>Remarks</th>
                                <th>Comments</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model.GetWorkFlowLokList != null && Model.GetWorkFlowLokList.Count > 0)
                            {
                                foreach (var item in Model.GetWorkFlowLokList)
                                {
                                    <tr>
                                        @*<td>@item.ApplicationRefNo</td>*@
                                        <td>@item.SubmissionDate</td>
                                        @if (item.vehicle_category_desc == "")
                                        {
                                            <td>Agency</td>
                                        }
                                        else
                                        {
                                            <td>@item.From</td>
                                        }
                                        <td>@item.Tooo</td>
                                        <td>@item.Remarks</td>
                                        <td>@item.comments</td>
                                        <td>Pending</td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </section>
