<script src="~/Scripts/DataTables/DataTables-1.10.20/js/jquery.dataTables.min.js"></script>

<div class="lms-registration-form" id="TeacherDetails">

    <nav id="navmenu">
        <div class="nav nav-tabs nav-fill" id="nav-tab" role="tablist">
