                        <h6 class="m-0 font-weight-bold text-primary mt-3"><b>Petitioner Details</b></h6>
                        <div class="row">
                            <table class="table table-striped table-bordered" id="petitioner_table">
                                <thead class="t_head">
                                    <tr>
                                        <th>SL. no.</th>
                                        <th>Name of the Petitioner</th>
                                        <th>Mobile No.</th>
                                        <th>Address</th>
                                        <th>Pincode</th>
                                    </tr>
                                </thead>
                                <tbody id="petitioner_table_body">
                                    @foreach (var data in Model.PetitionerList)
                                    {
                                        <tr>
                                            <td>@(slno++)</td>
                                            <td>@data.petitioner_name</td>
                                            <td>@data.petitioner_Mobile_no</td>
                                            <td>@data.petitioner_Address</td>
                                            <td>@data.pincode_Of_Petitioner</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>


                        <h6 class="m-0 font-weight-bold text-primary mt-3"><b>Respondent Details</b></h6>
                        <table class="table table-striped table-bordered" id="respondent_table">
                            <thead class="t_head">
                                <tr>
                                    <th>SL No.</th>
                                    <th>Name of Respondent</th>
                                    <th>Designation</th>
                                    <th>Department</th>
                                    <th>Agency Name</th>
                                    <th>Address</th>
                                    <th>Mobile No.</th>
                                    <th>Pincode</th>
                                </tr>
                            </thead>
                            <tbody id="respondent_tbody">
                                @foreach (var data in Model.RespondantList)
                                {
                                    <tr>
                                        <td>@(slnumber++)</td>
                                        <td>@data.Respondant_name</td>
                                        <td>@data.Respondant_designation</td>
                                        <td>@data.Respondant_department</td>
                                        <td>@data.Respondant_Agency_name</td>
                                        <td>@data.Respondant_address</td>
                                        <td>@data.Respondant_mobile</td>
                                        <td>@data.Respondant_pincode</td>
                                    </tr>
                                }
                            </tbody>
                        </table>


