                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="exampleFormControlInput1">
                                            Age of the person dead/injured
                                        </label>
                                        @Html.TextBox("occupation_injured", Model.CourtDetailsList[0].Age_of_injured, new { @class = "form-control" })                                    <label id="lbl_p_name" style="color:red" class="err" hidden>enter name of Age of dead/injured person</label>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="exampleFormControlInput1">
                                            Occupation of the person dead/injured
                                        </label>
                                        @Html.TextBox("occupation_injured", Model.CourtDetailsList[0].occupation_of_injured, new { @class = "form-control" })                                       <label id="lbl_p_name" style="color:red" class="err" hidden>enter name of Age of dead/injured person</label>
                                    </div>
                                </div>
                                <div class="col-md-5">
                                    <div class="form-group">
                                        <label for="exampleFormControlInput1">
                                            Name and address of the employer of the deceased or injured if any.
                                        </label>

                                        @Html.TextArea("Address_injured", Model.CourtDetailsList[0].Address_of_dead_details, new { @class = "form-control" })
                                    </div>
                                </div>

                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="exampleFormControlInput1">
                                            Monthly income of the person dead/injuries
                                        </label>
                                        @Html.TextBox("monthIncome", Model.CourtDetailsList[0].monthly_income_of_injured, new { @class = "form-control" })                                           <label id="lbl_income_dead" style="color:red" class="err" hidden>enter Monthly income of the person dead/injuries</label>
                                    </div>
                                </div>
                                <div class="col-md-9">
