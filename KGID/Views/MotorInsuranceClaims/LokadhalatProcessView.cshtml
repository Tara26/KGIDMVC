                                        <label id="lbl_p_Father_name" style="color:red" class="err" hidden>please enter valid Pincode</label>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="exampleFormControlInput1">Spouse Name</label>
                                        @Html.TextBox("spouse_name", Model.CourtDetailsList[0].Spouse_name, new { @class = "form-control" })                                    <label id="lbl_p_Address" style="color:red" class="err" hidden>Please enter Spouse Name</label>
