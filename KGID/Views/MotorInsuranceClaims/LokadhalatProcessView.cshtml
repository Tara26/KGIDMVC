                            <div class="card shadow-sm rounded p-3 mt-2">
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group row">
                                            <label class="col-sm-4 col-form-label align-center">
                                                MVC No. :
                                            </label>
                                            <div class="col-sm-8">
                                                @Html.TextBox("mvc_no", Model.CourtDetailsList[0].MVC_number, new { @class = "form-control" })                                                     <span id="lbl_mvc_no" class="err" style="color:red" hidden>Please Enter MVC No.</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group row">
                                            <label class="col-sm-4 col-form-label align-center">
                                                Name of Court :
                                            </label>
                                            <div class="col-sm-8">
                                                @Html.TextBox("NameOfCourt", Model.CourtDetailsList[0].Name_of_court, new { @class = "form-control" })

                                                <span id="lbl_name_of_court" class="err" style="color:red" hidden>Please Enter Court Name</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group row">
                                            <label class="col-sm-4 col-form-label align-center">
                                                Notice Date:
                                            </label>
                                            <div class="col-sm-7">
                                                @Html.TextBox("courtTime", Model.CourtDetailsList[0].CourtTime, new { @class = "form-control" })                                                   <span id="lbl_date_of_petition" class="err" style="color:red" hidden>Please Enter  Notice Date</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">

                                    <div class="col-md-4">
                                        <div class="form-group row">
                                            <label class="col-sm-4 col-form-label align-center">
                                                State :
                                            </label>
                                            <div class="col-sm-8">
                                                @Html.TextBox("Court_state", Model.CourtDetailsList[0].Court_state_name, new { @class = "form-control" })
                                                <span id="lbl_state_id2" class="err" style="color:red" hidden>Please Select State</span>
