@model KGID_Models.Claim.VM_ClaimRequiredDocuments

@{
    ViewBag.Title = "Upload Required Documets";
}

<style>
    .pt-12 {
        padding-top: 10px;
    }

    .m-t-10 {
        margin-top: 10px;
    }

    .btn-delete-doc {
        position: absolute;
        right: 0px;
        z-index: 2;
        top: 20px;
        cursor: pointer !important;
    }
</style>

<div class="row">
    <div class="col-lg-12">
        <div class="card shadow mb-4" style="width: 90%; margin: 0 auto;">
            <div class="card-header">
                <h6 class="m-0 font-weight-bold text-primary" style="padding-left: 15px;"><b>Documents Required</b></h6>
            </div>
            @using (Html.BeginForm("SumbitClaimDocuments", "Claims", FormMethod.Post, new { enctype = "multipart/form-data", id = "frmClaimDoc" }))
            {
                @Html.AntiForgeryToken()
                @Html.HiddenFor(m => m.EmpId, new { id = "hdnEmpId" })
            <div class="card-body">
                <h6>Please upload the scanned copies of all the documents required below</h6>
                <hr />

                <div class="row">
                    <div class="row col-6 ">

                        <label class="control-label col-6 pt-3 m-t-10 required">Death Certificate <span style="color:red">(*)</span>: </label>
                        <div class="col-6 pt-12">
                            <input type="file" class="form-control border-0 in-s" id="DeathCertificate" name="DeathCertificate" accept="application/pdf" , onchange="file_validation(this)" required />
                        </div>
                    </div>
                    <div class="row col-6">
                        <label class="control-label col-6 pt-3 m-t-10 required">Claim Application <span style="color:red">(*)</span>:</label>
                        <div class="col-6 pt-12">
                            <input type="file" class="form-control border-0" id="ClaimApplication" name="ClaimApplication" accept="application/pdf" , onchange="file_validation(this)" required />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="row col-6 ">

                        <label class="text-danger col-12 medium err-login" id="lbl_DeathCertificate" style="display:none"></label>
                        <div class="col-6 pt-12">
                            <label class="text-danger col-12 medium err-login" id="fil_DeathCertificate" style="display:none"></label>
                        </div>
                    </div>
                    <div class="row col-6">
                        <label class="text-danger col-12 medium err-login" id="lbl_ClaimApplication" style="display:none"></label>
                        <div class="col-6">
                            <label class="text-danger col-12 medium err-login" id="fil_ClaimApplication" style="display:none"></label>
                        </div>
                    </div>
