                    <span class="" id="spnVDOwnDamage">@Html.DisplayFor(model => model.mivd_own_damage_value)</span>
                </div>
                <div class="row col-sm-4">
                    <label class="control-label col-sm-4 pt-2-5" for="txtVDdateofmanu"><b>Premium Minimum Value   </b>: </label>
                    <span class="" id="spnVDMinValue">@Html.DisplayFor(model => model.mivd_vehicle_min_value)</span>
                </div>
                <div class="row col-sm-4">
                    <label class="control-label col-sm-4 pt-2-5" for="txtVDdateofmanu"><b>Premium Liability Value  </b>: </label>
