@model KGID_Models.KGIDMotorInsurance.VM_MotorInsuranceRenewalDetails.MotorInsuranceRenewalDetailsMI

<div class="form-group">
    <div class="row">
        <div class="row col-sm-6">
            <label class="control-label col-sm-4 pt-2-5" for="txtInitialAmt"><b>Policy Number </b>: </label>
            <span class="" id="spnInitialAmt">@Html.DisplayFor(m => m.MIPolicyNumber)</span>
        </div>
        <div class="row col-sm-6">
            <label for="txtRefNo" class="control-label col-sm-4 pt-2-5"><b>Previous Premium Amount </b>: </label>
            <span class="" id="spnRefNo">@Html.DisplayFor(m => m.MIPremium)</span>
        </div>
    </div>
</div>
<div class="form-group">
    <div class="row">
        <div class="row col-sm-6">
            <label class="control-label col-sm-4 pt-2-5" for="txtPurpose"><b>From Date </b>: </label>
            <span class="" id="spnPurpose">@Html.DisplayFor(m => m.MIPolicyFromDate)</span>
        </div>
        <div class="row col-sm-6">
            <label for="txtSubPurpose" class="control-label col-sm-4 pt-2-5"><b>To Date </b>: </label>
            <span class="" id="spnSubPurpose">@Html.DisplayFor(m => m.MIPolicyToDate)</span>
        </div>
    </div>
</div>
