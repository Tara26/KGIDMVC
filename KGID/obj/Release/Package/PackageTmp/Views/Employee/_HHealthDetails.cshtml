@model KGID_Models.NBApplication.VM_MOtherDetails

<div id="divHealthDetails">
    <form id="frmHealthDetails" enctype="multipart/form-data">
        @Html.HiddenFor(m => m.employee_id, new { id = "hdnEmployeeId" })
        @Html.HiddenFor(m => m.application_id, new { id = "hdnApplicationId" })
        <div class="form-group">
            <div class="row">
                <label for="txtWAS" class="knlan control-label col-12">
                    ಈ ಕೆಳಕಂಡ ಪ್ರಶ್ನೆಗಳಿಗೆ (ಪ್ರಶ್ನೆ ಸಂಖ್ಯೆ 4 ರಿಂದ 15 ರವರೆಗಿನ) ಉತ್ತರ ʼಹೌದುʼ ಎಂದಿದ್ದರೆ ಪೂರ್ಣ ವಿವರ ಕೊಡಿ (ಆರೋಗ್ಯ ಸುಸ್ಥಿತಿಯಲ್ಲಿಲ್ಲದಿದ್ದಲ್ಲಿ, ಅನಾರೋಗ್ಯದ ಬಗ್ಗೆ ಪೂರಕ
                    ದಾಖಲೆಗಳನ್ನು ಅಪ-ಲೋಡ್ ಮಾಡತಕ್ಕದ್ದು)
                </label>
                <label for="txtWAS" class="Enlan control-label col-12">
                    If the answer to following questions (Q. number 4 to 15) is “YES” then provide detailed information
                </label>
            </div>
            <div class="row">
                <label for="txtis" class="knlan control-label col-2">
                    4) ಎದೆ, ಹೃದಯ ಅಥವಾ ಶ್ವಾಸಕೋಶದಲ್ಲಿ ಅನಾರೋಗ್ಯದ ಯಾವುದಾದರೂ ಲಕ್ಷಣ ಕಂಡು ಬರುವುದೇ
                </label>
                <label for="txtis" class="Enlan control-label col-2">
                    4) Is there any symptoms of having
                    illness in chest, heart and lungs?
                </label>
                <label class="col-1">
                    @Html.RadioButtonFor(model => model.HavingIllnessinChest, true, new { @id = "rbtnHILYes", @class = "csHavingIllnessinChestY", onchange = "HDcheckboxchange(this.id,'txtHavingIllnessinChest')" })  Yes
                </label>
                <label class="col-1">
                    @Html.RadioButtonFor(model => model.HavingIllnessinChest, false, new { @id = "rbtnHILNo", @class = "csHavingIllnessinChestN", onchange = "HDcheckboxchange(this.id,'txtHavingIllnessinChest')" })  No
                </label>
                <div class="form-group col-4">
                    @Html.TextAreaFor(model => model.HavingIllnessinChestDesc, new { @id = "txtHavingIllnessinChest", @class = "form-control ClAlphaNum", rows = "3" })
                    <label id="errtxtHavingIllnessinChestReq" class="col-form-label-sm text-danger err" hidden>Please enter details about chest illness.</label>
                </div>
                <div class="form-group col-4 pl-0">
                    @if (!string.IsNullOrEmpty(Model.HavingIllnessinChestDocFileName))
                    {
                    <label class="knlan col-12">ಹಿಂದೆ ಅಪ್‌ಲೋಡ್ ಮಾಡಿದ ದಾಖಲೆಗಳು</label>
                        <label class="Enlan col-12">Previously uploaded Document</label>
                        @Html.TextBoxFor(m => m.HavingIllnessinChestDocFileName, new { @class = "display-as-label form-label col-12 doc-uploaded-HD ", @style = "display:none;" })
                        @*<a href="@Model.HavingIllnessinChestDocFileName" target="_blank" class="doc-uploaded-HD">Click Here</a>*@
                        @Html.ActionLink("Click Here", "ViewFilePath", "Home", new { FilePath = @Model.HavingIllnessinChestDocFileName }, htmlAttributes: new { @target = "_blank", @style = "text-decoration: underline; color: #0056b3;" })
                    }
                    @Html.TextBoxFor(m => m.HavingIllnessinChestDoc, new { @id = "txtHavingIllnessinChestDoc", type = "file", accept = "application/pdf", @class = "form-control display-as-label-upload fuHavingIllnessinChest txtAr-doc-health-req", onchange = "HDocFileChange(this.id,'errtxtHavingIllnessinChestDoc')" })
                    <label id="errtxtHavingIllnessinChestDoc" class="col-form-label-sm text-danger err" hidden>Please upload document for chest illness.</label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <label for="txtillness" class="knlan control-label col-2">
                    5) ಹಲ್ಲು, ಒಸಡು, ನಾಲಿಗೆ, ಕಿವಿ, ಮೂಗು, ಗಂಟಲು ಅಥವಾ ಕಣ್ಣುಗಳಲ್ಲಿ ಅನಾರೋಗ್ಯದ ಚಿಹ್ನೆ ಕಂಡು ಬರುವುದೇ?
                </label>
                <label for="txtillness" class="Enlan control-label col-2">
                    5) Is there any symptoms of disease in teeth
                    gums, tongue, ear, nose, Throat, eyes?
                </label>
                <label class="col-1">
                    @Html.RadioButtonFor(model => model.HavingIllnessinTeeth, true, new { @id = "rbtnHITYes", @class = "csHavingIllnessinTeethY", onchange = "HDcheckboxchange(this.id,'txtHavingIllnessinTeeth')" })  Yes
                </label>
                <label class="col-1">
