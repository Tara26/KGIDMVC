                    <label class="knlan col-12">ಹಿಂದೆ ಅಪ್‌ಲೋಡ್ ಮಾಡಿದ ದಾಖಲೆಗಳು</label>
                        <label class="Enlan col-12">Previously uploaded Document</label>
                        @Html.TextBoxFor(m => m.HavingIllnessinTeethDocFileName, new { @class = "display-as-label form-label col-12", @style = "display:none;" })
                        @*<a href="@Model.HavingIllnessinTeethDocFileName" target="_blank" class=" doc-uploaded-HD">Click Here</a>*@
                        @Html.ActionLink("Click Here", "ViewFilePath", "Home", new { FilePath = @Model.HavingIllnessinTeethDocFileName }, htmlAttributes: new { @target = "_blank", @style = "text-decoration: underline; color: #0056b3;" })
                    }
                    @Html.TextBoxFor(m => m.HavingIllnessinTeethDoc, new { @id = "txtHavingIllnessinTeethDoc", type = "file", accept = "application/pdf", @class = "form-control display-as-label-upload fuHavingIllnessinTeeth", onchange = "ODocFileSize(this.id,'errtxtHavingIllnessinTeethDoc')" })
                    <label id="errtxtHavingIllnessinTeethDoc" class="col-form-label-sm text-danger err" hidden></label>
